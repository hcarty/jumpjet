ifndef config
  config=debug64
endif
export config

PROJECTS := jumpjet

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

jumpjet: 
	@echo "==== Building jumpjet ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f jumpjet.make

clean:
	@${MAKE} --no-print-directory -C . -f jumpjet.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug64"
	@echo "   profile64"
	@echo "   release64"
	@echo "   bundle64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   jumpjet"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
